{
  "manifest_version": 2,
  "name": "Emojis table",
  "version": "1.0",

  "description": "Get the right emojis from your browser in any tab!",
  "homepage_url": "https://gitlab.com/louisgrasset/emojis-table",

  "browser_action": {
    "default_icon": {
      "16": "./icons/icon-16.png",
      "32": "./icons/icon-32.png"
    },
    "default_title": "Get the right emojis from your browser in any tab!",
    "default_popup": "popup.html"
  },

  "icons": {
    "16": "/icons/icon-16.png",
    "32": "/icons/icon-32.png",
    "48": "/icons/icon-48.png",
    "96": "/icons/icon-96.png",
    "128": "/icons/icon-128.png",
    "256": "/icons/icon-256.png"
  },

  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },

  "author": "Louis Grasset <hello@louisgrasset.fr> (louisgrasset.fr)"
}
